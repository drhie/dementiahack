

<div class="container">
  <div class="log-in">
  <div class="dashboard">
    <div class="form">
      <h1>log in</h1>
      <%= form_tag sessions_path, method: :post do %>
      <div class="field">
        <%= label_tag :email %>
        <%= text_field_tag :email %>
      </div>
      <div class="field">
        <%= label_tag :password %>
        <%= password_field_tag :password %>

      </div>

      <%= submit_tag do %>
        <div class="button"> Log in </div>
      <% end %>
      <% end %>
    </div>
  </div>

  </div>
</div>









